{
  ".heading": {
    "font-color": {
      "default": {
        "value": "{semantic.color.text.heading.default.value}",
        "type": "color"
      },
      "on-background": {
        "value": "{semantic.color.text.heading.on-background.value}",
        "type": "color"
      }
    },
    "font-family": {
      "serif": {
        "value": "{font-family.serif.value}",
        "type": "fontFamilies"
      },
      "sans-serif": {
        "value": "{font-family.sans-serif.value}",
        "type": "fontFamilies"
      },
      "mono": {
        "value": "{font-family.mono.value}",
        "type": "fontFamilies"
      }
    },
    "font-size": {
