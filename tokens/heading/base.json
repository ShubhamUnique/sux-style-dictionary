        "value": "{font-size.20.value}",
        "type": "fontSizes"
      },
      "l": {
        "value": "{font-size.17.value}",
        "type": "fontSizes"
      },
      "m": {
        "value": "{font-size.12.value}",
        "type": "fontSizes"
      },
      "s": {
